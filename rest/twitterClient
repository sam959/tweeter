
function getTweets(handle) => {
  const httpRes = fetch(`${host()}${path().replace("{cardCode}", cardCode)}`, {
    method: verb(),
    headers: headersPlainRO(),
  });
  const jsonRes = (await httpRes).json() as unknown as string;
  const codeRes = (await httpRes).status;
  switch (codeRes) {
    case 200:
      return jsonRes as unknown as CardStatus[];
    case 400:
      return [];
    default:
      return [];
  }
};